WHITESPACE = _{ " " | "\t" | "\n" }

subExpr = { addExpr ~ "-" ~ subExpr | addExpr }
addExpr = { mulExpr ~ "+" ~ addExpr | mulExpr }
mulExpr = { divExpr ~ "*" ~ mulExpr | divExpr }
divExpr = { powExpr ~ "/" ~ divExpr | powExpr }
powExpr = { factor ~ "^" ~ powExpr | factor }
factor = { "(" ~ subExpr ~ ")" | number }
number = { digit+ }
digit = {'0'..'9'}
